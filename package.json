{
  "name": "octal-back-provider",
  "displayName": "Octal Back-end Provider",
  "description": "Highlight javascript language in ES6 template literals",
  "version": "0.1.1",
  "publisher": "dtsf",
  "icon": "docs/octal.png",
  "engines": {
    "vscode": "^1.19.0"
  },
  "galleryBanner": {
    "color": "#363165",
    "theme": "dark"
  },
  "repository": {
    "url": "https://github.com/datsfilipe/octal-back-provider"
  },
  "bugs": {
    "url": "https://github.com/datsfilipe/octal-back-provider/issues"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "javascript",
    "es6",
    "lit-javascript",
    "octal",
    "octal-dev"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.ts"
        ],
        "scopeName": "inline.es6-javascript",
        "path": "./syntaxes/es6-inline-javascript.json",
        "embeddedLanguages": {
          "meta.embedded.block.javascript": "javascript"
        }
      }
    ]
  }
}
